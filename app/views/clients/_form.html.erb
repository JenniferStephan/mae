<%= simple_form_for(client) do |f| %>
  <%= f.input :first_name, label: 'Prénom du client' %>
  <%= f.input :last_name, label: 'Nom du client' %>
  <%= f.input :company_name, label: 'Nom de la société'  %>
  <%= f.input :company_address, label: 'Adresse du client' %>
  <%= f.input :email, label: 'Email du client' %>
  <%= f.input :company_siret, label: 'Siret du client' %>

  <%= f.submit "Valider", class: "button"  %>
<% end %>

